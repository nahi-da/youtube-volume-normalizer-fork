{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",

  "version": "6.0.1",
  "homepage_url": "https://github.com/nahi-da/youtube-volume-normalizer-fork",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "24": "icons/24.png",
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "ytvol-normalizer@nahi-da.com"
    }
  }
}